  "author": "gyuha shin",
  "build": {
    "appId": "cc.comics-craw"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "react-start": "craco start",
    "react-build": "craco build",
    "react-test": "craco test",
